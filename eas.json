{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "node": "20.18.0",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "resourceClass": "m-medium",
        "simulator": false
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      }
    },
    "preview": {
      "distribution": "internal",
      "prebuildCommand": "npx expo prebuild --clean"
    },
    "production": {
      "prebuildCommand": "npx expo prebuild --clean"
    }
  },
  "submit": {
    "production": {}
  }
}

